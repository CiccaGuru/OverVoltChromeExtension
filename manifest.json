{
  "manifest_version": 2,

  "name": "overVolt Extension",
  "description": "Con l'estensione ufficiale di overVolt supporti il canale con i tuoi acquisti. A te non costa nulla, per overVolt Ã¨ un grande aiuto.",
  "version": "1.2.5",
  "author": "CiccaGuru",
  "icons":
     {
        "128": "images/icon128.png",
        "16": "images/icon16.png",
        "48": "images/icon48.png"
  },
  "browser_action": {
    "default_icon": "images/iconDisabled.png",
    "default_popup": "popup.html"
  },
   "options_ui": {
     "page": "options.html",
     "chrome_style": false
     },
  "background": {
    "scripts": ["js/event.js"],
    "persistent": true
  },
  "permissions": [
    "tabs", "webRequest", "webRequestBlocking", "storage", "*://www.amazon.it/*", "*://www.banggood.com/*"
  ]
}
